<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_user"
    android:scrollbars="none">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context="com.zrkj.mes.zujiemian_activity"
        android:id="@+id/relativeLayout">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <!--分托-->
                <RelativeLayout
                android:id="@+id/relativeLayout_pymian1"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:clickable="true"

                >

                <ImageView
                    android:id="@+id/imageView_pymian1"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/fentuo" />

                <TextView
                    android:id="@+id/textView_pymian1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian1"
                    android:layout_centerHorizontal="true"
                    android:text="分  托"
                    android:gravity="center"
                    android:textSize="16sp" />
            </RelativeLayout>
                <!--入库复核-->
                <RelativeLayout
                    android:id="@+id/relativeLayout_pymian2"
                    android:layout_width="101dp"
                    android:layout_height="120dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:clickable="true">

                    <ImageView
                        android:id="@+id/imageView_pymian2"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/caigoufuhe" />

                    <TextView
                        android:id="@+id/textView_pymian2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView_pymian2"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="入库复核"
                        android:textSize="16sp" />
                </RelativeLayout>
                <!--入库质检-->
                <RelativeLayout
                    android:id="@+id/relativeLayout_pymian3"
                    android:layout_width="101dp"
                    android:layout_height="120dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:clickable="true">

                    <ImageView
                        android:id="@+id/imageView_pymian3"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/fentuo" />

                    <TextView
                        android:id="@+id/textView_pymian3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView_pymian3"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="入库质检"
                        android:textSize="16sp" />
                </RelativeLayout>
                <!--破袋调整-->
            </RelativeLayout>
        </ScrollView>
    </RelativeLayout>
    <!--破袋调整-->
    <RelativeLayout
        android:id="@+id/relativeLayout_pymian4"
        android:layout_width="101dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"
        android:layout_below="@+id/relativeLayout"
        android:clickable="true">

        <ImageView
            android:id="@+id/imageView_pymian4"
            android:layout_width="72dp"
            android:layout_height="72dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/fentuo" />

        <TextView
            android:id="@+id/textView_pymian4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_pymian4"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="破袋调整"
            android:textSize="16sp" />
    </RelativeLayout>
    <!--调拨-->
    <RelativeLayout
        android:id="@+id/relativeLayout_pymian5"
        android:layout_width="101dp"
        android:layout_height="120dp"

        android:layout_below="@+id/relativeLayout"
        android:layout_centerHorizontal="true"
        android:clickable="true">

        <ImageView
            android:id="@+id/imageView_pymian5"
            android:layout_width="72dp"
            android:layout_height="72dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/fentuo" />

        <TextView
            android:id="@+id/textView_pymian5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_pymian5"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="调拨"
            android:textSize="16sp" />
    </RelativeLayout>
    <!--销货-->
    <RelativeLayout
        android:id="@+id/relativeLayout_pymian6"
        android:layout_width="101dp"
        android:layout_height="120dp"

        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/relativeLayout"
        android:clickable="true">

        <ImageView
            android:id="@+id/imageView_pymian6"
            android:layout_width="72dp"
            android:layout_height="72dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/fentuo" />

        <TextView
            android:id="@+id/textView_pymian6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_pymian6"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="销货"
            android:textSize="16sp" />
    </RelativeLayout>
    <!--销货-->
    <RelativeLayout
        android:id="@+id/relativeLayout_pymian7"
        android:layout_width="101dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/relativeLayout_pymian4"
        android:clickable="true">

        <ImageView
            android:id="@+id/imageView_pymian7"
            android:layout_width="72dp"
            android:layout_height="72dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/fentuo" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_pymian7"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="退货"
            android:textSize="16sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout_pymian8"
        android:layout_width="101dp"
        android:layout_height="120dp"
        android:layout_below="@+id/relativeLayout_pymian5"
        android:layout_centerHorizontal="true"
        android:clickable="true">

        <ImageView
            android:id="@+id/imageView_pymian8"
            android:layout_width="72dp"
            android:layout_height="72dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/fentuo" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_pymian8"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="批号设置"
            android:textSize="16sp" />
    </RelativeLayout>
</RelativeLayout>



