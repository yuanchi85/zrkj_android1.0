<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bg_round_white"
    android:orientation="vertical">


    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="12dp"
        android:text="警告"
        android:textSize="30sp"
        android:textColor="@color/black"
        android:background="#82948e"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"/>

    <LinearLayout
        android:layout_marginTop="5dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/relativeLayout1_peisongxiangqing"
            android:layout_width="170dp"
            android:layout_height="25dp" >
            <TextView
                android:id="@+id/textView1_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:text="品号："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view5"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toEndOf="@+id/textView1_wuliaopeisong" />
            <TextView
                android:id="@+id/wuliaobianhao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_toEndOf="@+id/view5"
                android:layout_marginLeft="4dp"
                android:layout_toRightOf="@+id/textView1_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout2_peisongxiangqing"
            android:layout_width="300dp"
            android:layout_height="25dp"
            android:layout_centerHorizontal="true" >

            <TextView
                android:id="@+id/textView2_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="名称："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view3"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toEndOf="@+id/textView2_wuliaopeisong"/>
            <TextView
                android:id="@+id/wuliaomingcheng"
                android:layout_toEndOf="@+id/view3"
                android:layout_marginLeft="4dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_toRightOf="@+id/textView2_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/relativeLayout3_peisongxiangqing"
            android:layout_width="170dp"
            android:layout_height="25dp"
            >

            <TextView
                android:id="@+id/textView3_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="需领数量："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view4"
                android:layout_toEndOf="@+id/textView3_wuliaopeisong"
                android:layout_marginLeft="4dp"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="2dp"/>
            <TextView
                android:id="@+id/xunlingshuliang"
                android:layout_toEndOf="@+id/view4"
                android:layout_marginLeft="4dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_toRightOf="@+id/textView3_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout4_peisongxiangqing"
            android:layout_width="300dp"
            android:layout_height="25dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:id="@+id/textView4_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="可领数量："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view1"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toEndOf="@+id/textView4_wuliaopeisong"/>
            <TextView
                android:id="@+id/kelingshuliang"
                android:layout_toEndOf="@+id/view1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView4_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>

    </LinearLayout>




    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_height="40dp"
        android:layout_below="@+id/relativeLayout1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/RelativeLayout_marginTop"
        android:background="@color/background_user"
        tools:ignore="UnknownIdInLayout"
        android:lineSpacingExtra="3dp"
        android:layout_marginBottom="30dp">
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/shape_wihte_frame">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:lines="1"
                android:padding="1dp"
                android:text=""
                android:textSize="12sp" />

            <View
                android:id="@+id/view2"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toEndOf="@+id/textView2"
                android:background="#EEEFFF" />

            <EditText
                android:id="@+id/chanpintiaoma"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/view2"
                android:background="@drawable/transparent"
                android:ems="19"
                android:gravity="left|top"
                android:hint="请输入本次领料数量..."
                android:inputType="number"
                android:padding="1dp"
                android:selectAllOnFocus="true"
                android:singleLine="true"
                android:textColor="@color/black"
                android:textSize="12sp"
                tools:ignore="RtlHardcoded"/>

            <ImageView
                android:id="@+id/ImageView1"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="3dp"
                android:src="@drawable/shanchu"/>
        </RelativeLayout>
    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/cancel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:text="确定"
            android:textSize="20sp"
            android:background="@color/blue_touch_user"
            android:textColor="@color/black"/>


        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>


        <TextView
            android:id="@+id/submit"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="1.0"
            android:background="#e1dada"
            android:text="取消"
            android:textSize="20sp"
            android:textColor="@color/black"/>


    </LinearLayout>


</LinearLayout>